      border                = { 6, 6, 6, 6 }
      stretch                = TRUE
      orientation            = HORIZONTAL
      overlay_file            = "Scrollbars/scroll-thumb-horiz.png"
      overlay_stretch    = FALSE
      orientation            = HORIZONTAL
    }
    image
    {
      function                = SLIDER
